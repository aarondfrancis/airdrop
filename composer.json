{
    "name": "hammerstone/airdrop",
    "description": "A Laravel package to deploy your application faster by skipping asset compilation when possible.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Francis",
            "email": "aaron@hammerstone.dev"
        }
    ],
    "require": {
        "php": "^7.2|^8.0.2",
        "illuminate/support": "^7.0|^8.0|^9.0",
        "illuminate/filesystem": "^7.0|^8.0|^9.0",
        "illuminate/console": "^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0|^7.0",
        "mockery/mockery": "^1.3.3",
        "phpunit/phpunit": "^8.4|^9.5"
    },
    "autoload": {
        "psr-4": {
            "Hammerstone\\Airdrop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hammerstone\\Airdrop\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hammerstone\\Airdrop\\AirdropServiceProvider"
            ]
        }
    }
}
