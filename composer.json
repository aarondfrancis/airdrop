{
    "name": "aaronfrancis/airdrop",
    "description": "A Laravel package to deploy your application faster by skipping asset compilation when possible.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Francis",
            "email": "aaron@tryhardstudios.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "mockery/mockery": "^1.3.3",
        "phpunit/phpunit": "^8.4|^9.5|^10.5|^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "AaronFrancis\\Airdrop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AaronFrancis\\Airdrop\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AaronFrancis\\Airdrop\\AirdropServiceProvider"
            ]
        }
    }
}
